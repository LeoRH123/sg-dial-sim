{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./src",
		"module": "es2020",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"target": "ES2022",
		"typeRoots": ["node_modules/@types"],
		"lib": ["es2019", "dom"],
		"esModuleInterop": true,
		"useDefineForClassFields": false
	}
}
