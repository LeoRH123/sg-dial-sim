		"@angular/platform-browser": "15.2.6",
		"@angular/platform-browser-dynamic": "15.2.6",
		"@angular/router": "15.2.6",
		"@electron/remote": "2.0.9",
		"@ngrx/effects": "15.4.0",
		"@ngrx/store": "15.4.0",
		"@ngrx/store-devtools": "15.4.0",
		"electron-reloader": "1.2.3",
